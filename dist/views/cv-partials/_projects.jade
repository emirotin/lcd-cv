ul.media-list
  li.media
    .pull-left
      a.thumbnail(href="javascript:void(0)")
        img.media-object(src="/images/projects/likeandpay.png")
        span.preview
          span.glyphicon.glyphicon-zoom-in
    .media-body
      h2.media-heading
        | Like'n'Pay&#32;
        small FB like, but with payments
      p 1111

  li.media
    .pull-left
      #carousel-flux.carousel-flux.carousel.slide(data-ride="carousel")
        ol.carousel-indicators
         li.active(data-target="#carousel-flux", data-slide-to="0")
         li(data-target="#carousel-flux", data-slide-to="1")
         li(data-target="#carousel-flux", data-slide-to="2")

        .carousel-inner
          .item.active
            a.thumbnail(href="javascript:void(0)")
              img.media-object(src="/images/projects/flux1.png")
              span.preview
                span.glyphicon.glyphicon-zoom-in
          .item
            a.thumbnail(href="javascript:void(0)")
              img.media-object(src="/images/projects/flux2.png")
              span.preview
                span.glyphicon.glyphicon-zoom-in
          .item
            a.thumbnail(href="javascript:void(0)")
              img.media-object(src="/images/projects/flux3.png")
              span.preview
                span.glyphicon.glyphicon-zoom-in

    .media-body
      h2.media-heading
        | MTV&#32;
        small FB like, but with payments
      p 1111
